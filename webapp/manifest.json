{
	"_version": "1.1.0",
	"start_url": "start.html",
	"sap.app": {
		"_version": "1.1.0",
		"id": "ns1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.2.2"
		},
		"title": "{{app_title}}",
		"description": "{{app_description}}",
		"dataSources": {
			"servicerequest": {
				"uri": "/destinations/c4c/sap/byd/odata/v1/servicerequest/",
				"type": "OData",
				"destination": "/destinations/c4c/sap/byd/odata/v1/servicerequest/",
				"settings": {
					"localUri": "localService/metadata.xml",
					"annotations": [
						"servicerequestAnno"
					]
				}
			},
			"servicerequestAnno": {
				"uri": "localService/annotations2.xml",
				"type": "ODataAnnotation",
				"settings": {}
			}
		},
		"sourceTemplate": {
			"id": "OVP.cardtemplate",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"icons": {
			"icon": ""
		}
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ovp": {
					"minVersion": "1.31.0"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"servicerequest": {
				"dataSource": "servicerequest",
				"settings": {}
			}
		},
		"extends": {
			"component": "sap.ovp.app",
			"minVersion": "0.8.15",
			"extensions": {}
		}
	},
	"sap.ovp": {
		"globalFilterModel": "servicerequest",
		"globalFilterEntityType": "ServiceRequestCollection",
		"cards": {
			"card00": {
				"model": "servicerequest",
				"template": "sap.ovp.cards.list",
				"settings": {
					"title": "",
					"listType": "",
					"listFlavor": "",
					"sortBy": "",
					"sortOrder": "",
					"annotationPath": "com.sap.vocabularies.UI.v1.LineItem",
					"category": "card title",
					"description": "",
					"entitySet": "ServiceRequestFirstRequestedItemScheduleLineCollection"
				}
			},
			"card01": {
				"model": "servicerequest",
				"template": "sap.ovp.cards.stack",
				"settings": {
					"title": "HL",
					"category": "Title",
					"description": "STL",
					"entitySet": "ServiceRequestFirstRequestedItemScheduleLineCollection"
				}
			}
		}
	}
}